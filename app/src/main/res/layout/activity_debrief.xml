<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <RelativeLayout
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#15172C">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="15dp"
            android:id="@+id/menu"
            >

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:background="@drawable/coloringcross"
                android:clickable="false" />


        </LinearLayout>

        <TextView
            android:id="@+id/editprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="Debrief"
            android:textColor="#FFF"
            android:textSize="20sp"
            android:textStyle="normal"
            />
    </RelativeLayout>
    <com.guidiyam.sexrdv.customview.CustomScrollView
        android:id="@+id/scrv_bar"
        android:layout_below="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <LinearLayout
            android:id="@+id/main_layout"

            android:background="#15172C"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <LinearLayout
            android:weightSum="2"

            android:paddingLeft="50dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">



            <LinearLayout
                android:gravity="center_vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <TextView
                    android:textSize="12sp"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#767998"
                    android:text="Meeting details"/>
            </LinearLayout>

            </LinearLayout>
        <LinearLayout
            android:weightSum="2"
            android:paddingRight="30dp"
            android:paddingLeft="30dp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/rl_dt"
                android:layout_weight="1.5"
                android:layout_width="0dp"
                android:background="@drawable/mold_style"
                android:layout_height="35dp">

                <EditText
                    android:paddingLeft="15dp"
                    android:layout_centerVertical="true"
                    android:textColorHint="#EFEFEF"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@null"
                    android:id="@+id/edt_dt"
                    android:textColor="#FFF"
                    android:text="Wed, 18.03.2017"

                    />



            </RelativeLayout>


            <RelativeLayout
                android:layout_marginLeft="20dp"
                android:id="@+id/rl_time"
                android:layout_weight=".5"


                android:layout_width="40dp"
                android:background="@drawable/mold_style"
                android:layout_height="35dp">

                <EditText
                    android:paddingLeft="15dp"
                    android:layout_centerVertical="true"
                    android:textColorHint="#EFEFEF"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@null"
                    android:id="@+id/edttime"
                    android:textColor="#FFF"
                    android:text="18:30"

                    />



            </RelativeLayout>

            </LinearLayout>

        <LinearLayout
